{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",
    "removeComments": true,
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true,
    "strict": false,          
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true, 
  },
  "include": ["src/**/*"],
}
